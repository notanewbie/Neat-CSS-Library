		SetSquare("<a href='javascript:Do_One()'><white>One America News Network</white></a>", "oan.jpg", 4)
		SetSquare("<a href='javascript:Do_Eu()'><white>Euronews</white></a>", "eu.jpg", 5)
		SetSquare("<a href='javascript:Do_Sky()'><white>Sky News</white></a>", "sky.jpg", 6)
		
		SetSquare("<a href='javascript:Do_NBCN()'>NBC News</a>", "nbc_news.jpg", 7)
		SetSquare("<a href='javascript:Do_MSNBC()'>MSNBC</a>", "msnbc.jpg", 8)
		SetSquare("<a href='javascript:Do_CNN()'>CNN</a>", "cnn.jpg", 9)
		SetSquare("<a href='javascript:Do_FNC()'>Fox News</a>", "fnc.jpg", 10)
		SetSquare("<a href='javascript:Do_Newsmax()'>Newsmax TV</a>", "newsmax.jpg", 11)
		SetSquare("<a href='javascript:Do_CBSN()'>CBSN</a>", "cbsn-generic.jpg", 12)
		SetSquare("<a href='javascript:Do_ABC_News()'>ABC News Digital</a>", "abcn.jpg", 13)
		SetSquare("<a href='javascript:Do_ABC_News2()'>ABC News Digital 2</a>", "abcn.jpg", 14)
		SetSquare("<a href='javascript:Do_ABC_News3()'>ABC News Digital 3</a>", "abcn.jpg", 15)
		SetSquare("<a href='javascript:Do_ABC_News4()'>ABC News Digital 4</a>", "abcn.jpg", 16)
		SetSquare("<a href='javascript:Do_ABC_News5()'>ABC News Digital 5</a>", "abcn.jpg", 17)
	}
	if(type == "pol") {
		ClearBoxes()
		SetSquare("<a href='javascript:Do_Vice()'>Vice News</a>", "vice.jpg", 3)
		SetSquare("<a href='javascript:Do_LTV()'><white>Levin TV</white></a>", "levin.jpg", 4)
		SetSquare("<a href='javascript:Do_TYT()'><white>The Young Turks</white></a>", "tyt.jpg", 5)
		SetSquare("<a href='javascript:Do_One()'><white>One America News Network</white></a>", "oan.jpg", 6)
		SetSquare("<a href='javascript:Do_Blaze()'>TheBlaze</a>", "blaze.jpg", 7)
		SetSquare("<a href='javascript:Do_NBCN()'>NBC News</a>", "nbc_news.jpg", 8)
		SetSquare("<a href='javascript:Do_MSNBC()'>MSNBC</a>", "msnbc.jpg", 9)
		SetSquare("<a href='javascript:Do_CNN()'>CNN</a>", "cnn.jpg", 10)
		SetSquare("<a href='javascript:Do_FNC()'>Fox News</a>", "fnc.jpg", 11)
		SetSquare("<a href='javascript:Do_Newsmax()'>Newsmax TV</a>", "newsmax.jpg", 12)
		SetSquare("<a href='javascript:Do_CBSN()'>CBSN</a>", "cbsn-generic.jpg", 13)
		}
}
function ClearBoxes() {
	SetSquare("", "", 3)
	SetSquare("", "", 4)
	SetSquare("", "", 5)
	SetSquare("", "", 6)
	SetSquare("", "", 7)
	SetSquare("", "", 8)
	SetSquare("", "", 9)
	SetSquare("", "", 10)
	SetSquare("", "", 11)
	SetSquare("", "", 12)
	SetSquare("", "", 13)
	SetSquare("", "", 14)
	SetSquare("", "", 15)
	SetSquare("", "", 16)
	SetSquare("", "", 17)
}
Boxes("default")
function CheckMenu() {
	Menu = document.getElementsByTagName("select")
	index = Menu[0].selectedIndex;
	Menu = Menu[0]
	if(Menu.options[index].text == "Featured") {
		Boxes("default")
	}
	if(Menu.options[index].text == "Live") {
		Boxes("live")
	}
	if(Menu.options[index].text == "YouTube") {
		Boxes("yt")
	}
	if(Menu.options[index].text == "News") {
		Boxes("news")
	}
	if(Menu.options[index].text == "Politics") {
		Boxes("pol")
	}
}


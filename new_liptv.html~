state = "stopped"
function handle() {
	if(state == "hidden") {
		ShowVideo()
	}
	else if(state == "shown") {
		HideVideo()
	}
}
function ShowVideo() {
	toptext = document.getElementsByTagName("top_text");
	toptext[0].id = "";
	bottext = document.getElementsByTagName("bottom_text");
	bottext[0].id = "";
	menu = document.getElementById("menu");
	menu.style.visibility = "hidden";
	vidpart = document.getElementsByTagName("vid_part");
	vidpart[0].id = ""
	botsquare1 = document.getElementsByTagName("bot_square1");
	botsquare1[0].id = "";
	botsquare2 = document.getElementsByTagName("bot_square2");
	botsquare2[0].id = "";
	botsquare3 = document.getElementsByTagName("bot_square3");
	botsquare3[0].id = "";
	botsquare4 = document.getElementsByTagName("bot_square4");
	botsquare4[0].id = "";
	botsquare5 = document.getElementsByTagName("bot_square5");
	botsquare5[0].id = "";
	state = "shown"
}
function HideVideo() {
	menu = document.getElementsByTagName("span");
	menu[0].id = "";
	vidpart = document.getElementsByTagName("vid_part");
	toptext = document.getElementsByTagName("top_text");
	toptext[0].id = "hidden";
	bottext = document.getElementsByTagName("bottom_text");
	bottext[0].id = "hidden";
	botsquare1 = document.getElementsByTagName("bot_square1");
	botsquare1[0].id = "hidden";
	botsquare2 = document.getElementsByTagName("bot_square2");
	botsquare2[0].id = "hidden";
	botsquare3 = document.getElementsByTagName("bot_square3");
	botsquare3[0].id = "hidden";
	botsquare4 = document.getElementsByTagName("bot_square4");
	botsquare4[0].id = "hidden";
	botsquare5 = document.getElementsByTagName("bot_square5");
	botsquare5[0].id = "hidden";
	//vidpart[0].id = "hidden";
	state = "hidden"
}
function StartVideo(url) {
	toptext = document.getElementsByTagName("top_text");
	toptext[0].id = "";
	bottext = document.getElementsByTagName("bottom_text");
	bottext[0].id = "";
	botsquare1 = document.getElementsByTagName("bot_square1");
	botsquare1[0].id = "";
	botsquare2 = document.getElementsByTagName("bot_square2");
	botsquare2[0].id = "";
	botsquare3 = document.getElementsByTagName("bot_square3");
	botsquare3[0].id = "";
	botsquare4 = document.getElementsByTagName("bot_square4");
	botsquare4[0].id = "";
	botsquare5 = document.getElementsByTagName("bot_square5");
	botsquare5[0].id = "";
	menu = document.getElementById("menu");
	span = document.getElementsByTagName("span");
	if(menu) {
		menu = menu;
	}
	else {
		menu = span[0];
	}
	menu.id = "hidden";
	vidpart = document.getElementsByTagName("vid_part");
	vidpart[0].style.visibility = "visible";
	vidpart[0].id = "";
	iframe = document.getElementsByTagName("iframe")
	toppart = document.getElementsByTagName("top_part");
	toppart[0].id = "";
	bottompart = document.getElementsByTagName("bottom_part");
	bottompart[0].id = "";
	if(iframe[0]) {
		if(iframe[0].src == url) {
		}
		else {
			vidpart[0].innerHTML = "<iframe src='" + url + "'></iframe>"
		}
	}
	else {
		vidpart[0].innerHTML = "<iframe src='" + url + "'></iframe>"
	}
	iframe[0].style.width = "100%";
	if(url == "http://www.cbsnews.com/live/") {
		iframe[0].width = "70px";
	}
	state = "shown"
	if(url == "http://www.cbsnews.com/live/compact/") {
		iframe[0].style.width = "50%";
	}
}
